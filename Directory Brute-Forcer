import requests

def dir_bruter(target_url, wordlist):
    print(f"Brute-forcing {target_url}...")
    for word in wordlist:
        url = f"{target_url}/{word}"
        try:
            response = requests.get(url, timeout=1)
            # 200 means the page exists, 403 means forbidden but exists
            if response.status_code == 200:
                print(f"[!] Found: {url}")
        except requests.exceptions.RequestException:
            pass

# Usage
target = "http://example.com"
common_dirs = ["admin", "login", "uploads", "db", "api", "v1"]
dir_bruter(target, common_dirs)
